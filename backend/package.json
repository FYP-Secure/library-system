    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "nodeman": "^1.1.2",
    "nodemon": "^2.0.16",
    "prisma": "^3.15.1",
    "ts-node": "^10.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.42",
